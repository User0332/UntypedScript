import { NORMAL_ARRAY_BYTE_OFFSET } from "uts-info"

const value_at = (arr, i) => {
	return deref( arr+(i*NORMAL_ARRAY_BYTE_OFFSET) )
}

const foreach_slow =  (arr, len, callback) => {
	let i = 0
	
	while (i < len)
	{
		callback(value_at(arr, i), i)

		i+=1
	}

	return 0
}

export {
	value_at,
	foreach_slow
}